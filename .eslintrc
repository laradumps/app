{
    "env": {
        "browser": true,
        "es2021": true
    },
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["vue", "@typescript-eslint", "prettier"],
    "parser": "@typescript-eslint/parser",
    "extends": ["plugin:vue/vue3-essential", "eslint:recommended", "plugin:@typescript-eslint/eslint-recommended", "plugin:@typescript-eslint/recommended", "plugin:prettier/recommended"],
    "rules": {
        "no-console": "warn",
        "@typescript-eslint/ban-ts-comment": "off",
        "prettier/prettier": "error",
        "@typescript-eslint/no-var-require": "off"
    }
}
